{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": ["element-plus/global", "@types/jquery", "draggabilly"],
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@examples/*": ["src/examples/*"]
    },
    "jsxImportSource": "react"
  }
}
